{
  "name": "javascript-test-snippets",
  "description": "Code snippets for JavaScript testing frameworks in ES6 syntax",
  "version": "1.4.0",
  "displayName": "JavaScript Test Snippets",
  "publisher": "JaredE",
  "icon": "images/jasmine.png",
  "license": "SEE LICENSE IN LICENSE.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/JaredCE/vscode-test-snippets"
  },
  "engines": {
    "vscode": "0.10.x"
  },
  "main": "./extension.js",
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "configuration": {
      "title": "Javascript Test Frameworks",
      "properties": {
        "javascript-test-snippets.framework": {
          "id": "javascript-test-snippets.framework",
          "type": "string",
          "description": "Choose the JavaScript testing framework you most often use",
          "default": "mocha",
          "enum": [
            "mocha",
            "jasmine",
            "jest"
          ]
        },
        "javascript-test-snippets.quoteStyle": {
          "id": "javascript-test-snippets.quoteStyle",
          "description": "Choose your preferred quoteStyle",
          "type": "string",
          "default": "\"",
          "enum": [
            "\"",
            "'",
            "`"
          ]
        }
      }
    }
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "devDependencies": {
    "@types/vscode": "^1.74.0"
  },
  "dependencies": {
    "lodash.merge": "^4.6.2"
  }
}
